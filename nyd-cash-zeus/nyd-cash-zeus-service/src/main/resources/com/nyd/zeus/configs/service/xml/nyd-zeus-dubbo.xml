	<dubbo:service ref="zeusForZLQServise" interface="com.nyd.zeus.api.zzl.ZeusForZLQServise" protocol="dubbo" />
	<dubbo:service ref="zeusForWHServise" interface="com.nyd.zeus.api.zzl.ZeusForWHServise" protocol="dubbo" />
