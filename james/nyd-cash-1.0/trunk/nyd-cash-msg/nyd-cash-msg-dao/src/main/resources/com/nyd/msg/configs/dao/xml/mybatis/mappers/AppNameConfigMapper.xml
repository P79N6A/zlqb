<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.nyd.msg.dao.mapper.AppNameConfigMapper">

    <select id="selectNameByCode" parameterType="string" resultType="string">
        select app_name_value from xxd_msg.app_name_config
        where app_name_code = #{appNameCode}
    </select>

</mapper>